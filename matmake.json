[
  {
    "name": "matmake2-core",
    "src": "src/*.cpp"
  },
  {
    "name": "matmake2",
    "in": "@matmake2-core",
    "src": "src/main/main.cpp",
    "command": "[exe]",
    "copy": "demos/"
  }
  {
    "name": "task_test",
    "in": "@matmake2-core",
    "out": "matmake2",
    "command": "[test]"
  },
  {
    "name": "all",
    "config": [
      "c++17"
    ],
    "includes": [
      "src",
      "lib/json.h/include"
    ],
    "pch": "src/precompiled.h"
  }
]
