[
  {
    "name": "pch",
    "in": "[content]",
    "content": "#include <iostream>\n#include<vector>\n"
  },
  {
    "in": "data/Readme.md",
    "out": ".",
    "command": "[copy]"
  },
  {
    "src": "src/*.cpp", // Ska skapas implicit
    "depfile": "{name}.o.d",
    "out": [ "{name}.o" ],
    "command": "[compile]"
  },
  {
    "name": "main",
    "in": [ "src/*.cpp" ],
    "out": "main.exe",
    "dir": "build",
    "flags": "{parent.flags} -Wall",
    "pch" : "@pch",
    "includes": [
      "include"
    ],
    "sysincludes": [
      "bla"
    ],
    "command": "[link]"
  },
  {
    "name": "all",
    "in": [ "@main" ],
    "config": [
      "c++17"
    ]
  },
  {
    "name": "g++",
    "in": [ "@all" ],
    "compiler": "g++",
  }
]
