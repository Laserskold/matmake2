[
  {
    "out": "src/main.cpp"
  },
  {
    "out": "src/other.cpp"
  },
  {
    "out": "data/Readme.md"
  },
  {
    "out": "src/precompiled.hpp"
  },
  {
    "out": "src/precompiled.hpp.pch",
    "in": "src/precompiled.hpp",
    "command": "[pch]"
  },
  {
    "in": "data/Readme.md",
    "out": "Readme.md",
    "command": "[copy]"
  },
  {
    "src": "src/main.cpp",
    "in": [ "src/precompiled.hpp.pch", "src/main.cpp" ],
    "depfile": "main.o.d",
    "out": "main.o",
    "command": "[compile]"
  },
  {
    "src": "src/other.cpp",
    "in": [ "src/precompiled.hpp.pch", "src/other.cpp" ],
    "depfile": "other.o.d",
    "out": "other.o",
    "command": "[compile]"
  },
  {
    "name": "main",
    "in": [ "main.o", "other.o" ],
    "out": "main.exe",
    "dir": "build",
    "flags": "-Wall",
    "type": "exe",
    "includes": [
      "include"
    ],
    "sysincludes": [
      "bla"
    ],
    "command": "[link]"
  },
  {
    "name": "all",
    "in": [ "@main" ],
    "config": [
      "c++17"
    ]
  },
  {
    "name": "g++",
    "in": [ "@all" ],
    "dir": "g++",
    "cxxflags": "-Wall -Wpedantic -Wextra",
    "compiler": "g++",
    "commands": {
      "compile": "g++ {in} -o {out} -MMD -MT {depfile} {cxxflags} {flags} {includes}",
      "link": "g++ {in} -o {out} {ldflags} {includes}",
      "pch": "g++ {in} -o {out} -MMD -MT {depfile} {cxxflags} {flags} {includes}"
    }
  }
]
